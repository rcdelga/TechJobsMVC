<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

    @{int count = 0; }
    @foreach (Dictionary<string, string> dict in ViewBag.jobs)
    {
        count++;
        
        <h2>@count</h2>
        <table class="job-listing">
            @foreach (KeyValuePair<string, string> kvp in dict)
            {
                <tr>
                    <td>@kvp.Key</td>
                    <td>@kvp.Value</td>
                </tr>
            }
        </table>
    }